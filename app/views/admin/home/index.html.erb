<h1>Hi!</h1>

<% if user_signed_in? %>
  <%= button_to "Log Out", destroy_user_session_path, { onclick: "return confirm('Are you sure you want to delete?')", method: :delete, class: "btn btn-secondary" } %>
<% else %>
  <%= link_to "Login", new_user_session_path, class: "btn btn-secondary" %>
<% end %>