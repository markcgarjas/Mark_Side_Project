<div class="d-flex">
  <div class="col-auto text-center">
    <h1>Users</h1>
    <table class="table table-responsive-sm table-hover table-bordered">
      <thead>
      <tr>
        <th>Parent Email</th>
        <th>Email</th>
        <th>Total Deposit</th>
        <th>Member Total Deposits</th>
        <th>Coins</th>
        <th>Total Used Coins</th>
        <th>Children Members</th>
        <th>Phone</th>
      </tr>
      </thead>
      <% @users.each do |user| %>
        <tbody>
        <tr>
          <td><%= user.parent&.email %></td>
          <td><%= user.email %></td>
          <td><%= user.total_deposit %></td>
          <td><% "#" %></td>
          <td><%= user.coins %></td>
          <td><% "#" %></td>
          <td><%= user.childer_members %></td>
          <td><%= user.phone %></td>
        </tr>
        </tbody>
      <% end %>
    </table>
  </div>
</div>